CREATE DATABASE `estoque` /*!40100 DEFAULT CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci */ /*!80016 DEFAULT ENCRYPTION='N' */;

CREATE TABLE `categorias` (
  `categoriaid` bigint NOT NULL AUTO_INCREMENT,
  `embalagem` varchar(255) NOT NULL,
  `nome` varchar(255) NOT NULL,
  `status` varchar(255) NOT NULL,
  `tamanho` varchar(255) NOT NULL,
  PRIMARY KEY (`categoriaid`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE `entradas` (
  `quantidade` int DEFAULT NULL,
  `categoria_ide` bigint DEFAULT NULL,
  `data_hora` datetime(6) DEFAULT NULL,
  `id` bigint NOT NULL AUTO_INCREMENT,
  `produto_id` bigint DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FKbl01iuw3e2f8q446xjs5gw4s6` (`categoria_ide`),
  KEY `FKlu41qhk6wwt5sb05i0y3xen9q` (`produto_id`),
  CONSTRAINT `FKbl01iuw3e2f8q446xjs5gw4s6` FOREIGN KEY (`categoria_ide`) REFERENCES `categorias` (`categoriaid`),
  CONSTRAINT `FKlu41qhk6wwt5sb05i0y3xen9q` FOREIGN KEY (`produto_id`) REFERENCES `produtos` (`produtoid`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE `movimentacao_estoque` (
  `quantidade` int NOT NULL,
  `id` bigint NOT NULL AUTO_INCREMENT,
  `produto_id` bigint NOT NULL,
  `tipo` varchar(255) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `FKaigqdx3dtvvi0jl33684mplob` (`produto_id`),
  CONSTRAINT `FKaigqdx3dtvvi0jl33684mplob` FOREIGN KEY (`produto_id`) REFERENCES `produtos` (`produtoid`)
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE `produtos` (
  `preco` decimal(10,2) NOT NULL,
  `quantidade` int DEFAULT NULL,
  `quantidade_minima` int NOT NULL,
  `categoria_id` bigint NOT NULL,
  `produtoid` bigint NOT NULL AUTO_INCREMENT,
  `nome` varchar(255) NOT NULL,
  `status` varchar(255) NOT NULL,
  PRIMARY KEY (`produtoid`),
  KEY `FK8rqw0ljwdaom34jr2t46bjtrn` (`categoria_id`),
  CONSTRAINT `FK8rqw0ljwdaom34jr2t46bjtrn` FOREIGN KEY (`categoria_id`) REFERENCES `categorias` (`categoriaid`)
) ENGINE=InnoDB AUTO_INCREMENT=33 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

CREATE TABLE `saidas` (
  `quantidade` int DEFAULT NULL,
  `categoria_ide` bigint DEFAULT NULL,
  `data_hora` datetime(6) DEFAULT NULL,
  `id` bigint NOT NULL AUTO_INCREMENT,
  `produto_id` bigint DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FKlssmp4rdjla1yo7m3qs8mnlof` (`categoria_ide`),
  KEY `FK8eh5m76ug49feb5f13u2stkg7` (`produto_id`),
  CONSTRAINT `FK8eh5m76ug49feb5f13u2stkg7` FOREIGN KEY (`produto_id`) REFERENCES `produtos` (`produtoid`),
  CONSTRAINT `FKlssmp4rdjla1yo7m3qs8mnlof` FOREIGN KEY (`categoria_ide`) REFERENCES `categorias` (`categoriaid`)
) ENGINE=InnoDB AUTO_INCREMENT=27 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
